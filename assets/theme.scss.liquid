/*================ Slick Slider SCSS ================*/
.slick-slider {
  position: relative;
  display: block;
  box-sizing: border-box;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -ms-touch-action: pan-y;
  touch-action: pan-y;
  -webkit-tap-highlight-color: transparent;
}
.slick-list {
  position: relative;
  overflow: hidden;
  display: block;
  margin: 0;
  padding: 0;

  &:focus {
    outline: none;
  }

  &.dragging {
    cursor: pointer;
    cursor: hand;
  }
}
.slick-slider .slick-track,
.slick-slider .slick-list {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.slick-track {
  position: relative; left:0; top:0; display:block;

  &:before,
  &:after { content:""; display:table; }
  &:after { clear:both; }

  .slick-loading & { visibility: hidden; }
  .grid__item { clear:none !important; text-align:center; }
}
.slick-slide {
  float: left;
  height: 100%;
  min-height: 1px;
  [dir="rtl"] & {
    float: right;
  }
  
  img { display:block; }
  video { max-width: 100%; }
  .product-single__media {
    position: relative;
    
    >video,
    >iframe { 
      position: absolute;
      left: 0;
      right: 0;
      bottom: 0;
      top: 0;
      width: 100%;
      height: 100%;
    }
  }
  
  &.slick-loading img { display:none; }

  display:none;

  &.dragging img { pointer-events:none; }

  .slick-initialized & { display:block; }
  .slick-loading & { visibility:hidden; }
  .slick-vertical & { display:block; height:auto; border:1px solid transparent; }
}
.slick-arrow.slick-hidden { display:none; }

