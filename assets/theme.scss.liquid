  	.slick-next { right:15px; }
  }
  
  @include media-query($widescreen-up)
      {
      .slick-arrow { opacity:0; @include transition(all 0.5s ease-in-out); }
       &:hover .slick-arrow { opacity:1;}       
   }
  
