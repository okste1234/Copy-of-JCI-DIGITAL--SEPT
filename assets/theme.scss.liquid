/// Collection List
.collection-box { clear:both;  
  .collection-grid-item { padding-bottom:0; margin-bottom:0; }
  
  .collection-grid-item__link { position:relative; display:block; border:none; overflow:hidden;
    img { margin:0 auto; width:100%; display:block; }
  } 
  .collection-grid-item__link:before { @include transition(all 0.5s ease-in-out); content: ""; position: absolute; top: -110%; left: -210%; width: 200%; height: 200%; opacity: 0; filter: alpha(opacity=0); 
    -webkit-transform: rotate(30deg); -ms-transform: rotate(30deg);
    -moz-transform: rotate(30deg); -o-transform: rotate(30deg); 
    transform: rotate(30deg); 
    background: rgba(255,255,255,0.10); 
    background: linear-gradient(to right,rgba(255,255,255,0.10) 0%,rgba(255,255,255,0.10) 77%,rgba(255,255,255,0.5) 92%,rgba(255,255,255,0) 100%);
  }
  
  .collection-grid-item__link:hover:before { opacity: 1; filter: alpha(opacity=100); top: -40%; left: -40%; 
     @include transition(all 0.5s ease-in-out); }
  
  
  .collection-grid-item__title-wrapper { text-align:center; position:absolute; bottom:15px; width:100%;
    &:before { display:none;}
      @include media-query($small){
        bottom:0;
      }
