/*================ #Helper Classes ================*/
/* ===========================================================
   Basic Styles
 * ===========================================================*/
.page-width {
  @include clearfix();
  max-width: $width-site;
  margin: 0 auto;
}

.main-content {
  @include media-query($medium-up) { min-height:500px; }
}

.template-index {  
  .main-content {
    padding-top: $section-spacing-small;

    @include media-query($medium-up) {
      padding-top: $section-spacing;
    }
   }
}

.section-header {
  margin-bottom: $section-spacing-small;

  @include media-query($medium-up) {
    margin-bottom: $section-spacing
  }
  h2 { font-weight:{{ settings.section-font-weight}};}
}

/// Typography
input,
textarea,
button,
select {
  font-size: $font-size-base;
  font-family: $font-stack-body;
  color: $color-text;
  line-height: 1.5;
}

// Prevent zoom on touch devices in active inputs
@include media-query($medium-down) {
  input,
  textarea,
  select,
  button {
    font-size: $font-size-mobile-input;
  }
}

/// Headings
h1,h2,h3,h4,h5,h6 {
  margin: 0 0 ($section-spacing-small / 2);
  font-family: $font-stack-header;
  font-weight: $font-weight-header;
  line-height: 1.2;
  overflow-wrap: break-word;
  word-wrap: break-word;
  color:$color-text;
  a {
    color: inherit;
    text-decoration: none;
    font-weight: inherit;
  }
}

h1 {
  font-size: em(floor($font-size-header * 1.4)); letter-spacing:0; text-transform: {{settings.header_case}};
  @include media-query($small) { font-size: em(floor($font-size-header * 1.25)); }
}

h2 {
  font-size: em(floor($font-size-header * 1));
  text-transform: {{settings.header_case}};
  letter-spacing: 0.03em;

  @include media-query($small) { font-size: em(floor($font-size-header * 0.8)); }
}

h3 {
  font-size: em(floor($font-size-header * 0.85));
  text-transform:{{settings.header_case}};
  @include media-query($small) { font-size: em(floor($font-size-header * 0.7)); }
}

h4 {
  font-size: em(floor($font-size-header * 0.75));
  @include media-query($small) { font-size: em(floor($font-size-header * 0.6)); }
}

h5 {
  font-size: em(floor($font-size-header * 0.65));
  @include media-query($small) { font-size: em(floor($font-size-header * 0.5)); }
}

h6 {
  font-size: em(floor($font-size-header * 0.55));
  @include media-query($small) { font-size: em(floor($font-size-header * 0.4)); }
}

.h1 {
  @extend h1;
}

.h2 {
  @extend h2;
}

.h3 {
  @extend h3;
}

.h4 {
  @extend h4;
}

.h5 {
  @extend h5;
}

.h6 {
  @extend h6;
}

/// RTE headings
.rte {
  color: $color-body-text;
  margin-bottom: $section-spacing-small;

  a { color:{{settings.color_links}}; }
  // If an .rte div is the last element in its parent,
  // make it flush with the bottom of the container
  &:last-child {
    margin-bottom: 0;
  }

  h1,h2, h3, h4, h5, h6 {
    margin-top: $gutter-site;
    margin-bottom: $gutter-site / 2;

    &:first-child {
      margin-top: 0;
    }
  }

  li {
    margin-bottom: 4px;
    list-style: inherit;

    &:last-child {
      margin-bottom: 0;
    }
  }
}

// rte setting type to act like a <p> tag
.rte-setting {
  margin-bottom: $section-spacing-small / 1.8; // same as p

  &:last-child {
    margin-bottom: 0;
  }
}

@include media-query($small) {
	p { font-size: em($font-size-base - 1); }
}

/// Hero and slideshow headers
.mega-title,
.mega-subtitle { text-shadow: 0 0 15px $color-text-shadow; }

.mega-title {
  margin-bottom: 8px;  
   font-size: em($font-size-header - 2);
  @include media-query($medium-up) {
     font-size: em($font-size-header);
  }
  
}

.mega-title--large {
  font-size: em($font-size-header - 2);

  @include media-query($medium-up) {
    font-size: em($font-size-header + 2);
  }
  
   @include media-query($large-up) {
      font-size: em(floor($font-size-header * 1.2));
   }
  
   @include media-query($widescreen) {
      font-size: em(floor($font-size-header * 1.8));
   }
}

.mega-subtitle {
  @include media-query($medium-up) {
    font-size: em($font-size-base + 4);
    margin: 0 auto;
    max-width: 90%;
  }
  p { color:inherit; }
}

.mega-subtitle--large {
  font-size: em($font-size-base + 4);
  font-weight: $font-weight-header;

  @include media-query($medium-up) {
    font-size: em($font-size-base + 6);
  }
}

/* ===========================================================
   Rich Text Editor
 * ===========================================================*/


/// Links and Buttons
a { text-decoration:none; @include transition(all 0.3s ease-in-out);
  &:hover,
  &:focus { opacity: $opacity-link-hover; }

  &.classic-link {
    text-decoration: underline;
  }
}

// Buttons
.btn, .shopify-payment-button__button--unbranded {
  @include user-select();
  @include prefix(appearance, none, webkit moz spec);
  display: inline-block;
  width: auto; height:auto;
  text-decoration: none;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
  border: 1px solid transparent;
  border-radius: 0;
  padding: $input-padding-top-bottom-small $input-padding-left-right-small $input-padding-top-bottom-small;
  background-color: $color-btn-primary;
  color: $color-btn-primary-text;
  font-family: $font-stack-body;
  font-weight: 400;
  text-transform: uppercase;
  letter-spacing: $letter-spacing;
  line-height:normal;
  white-space: normal;
  font-size: 13px;
  @include transition(all 0.3s ease-in-out);
  

  @include media-query($medium-up) {
    padding: $input-padding-top-bottom-small $input-padding-left-right $input-padding-top-bottom-small;
  }

  &:hover,
  &:focus {
     background-color:lighten($color-btn-primary,10%);
     opacity:0.9;
     color:$color-btn-primary-text; text-decoration:none;
  }

  .icon-arrow-right, .icon-arrow-left { height:9px; }

  &[disabled] {
    cursor: default;
    opacity: 0.5;
    background-color: $color-btn-primary;
    color: $color-btn-primary-text;
  }
}

.btn--secondary {
  background-color: $color-small-button;
  color: $color-small-button-text; 
  border:1px solid $color-small-button-text; 
  &:hover,
  &:focus{  color: $color-btn-primary-text; background-color:$color-btn-primary; }
}

.btn--small {
  font-family: $font-stack-body;
  padding: 8px 10px;
  font-size: em(12);
  line-height: 1;
}

// Button variations
@include media-query($small) {
  .btn--small-wide {
    padding-left: 50px;
    padding-right: 50px;
  }
}

.btn--link {
  background-color: transparent;
  border: 0;
  margin: 0;
  color: {{settings.color_links}};
  text-align: left; text-decoration:none;
  outline:none !important;
  box-shadow:none !important;

  &:hover,
  &:focus { opacity: $opacity-link-hover; text-decoration:none; }

  .icon {
    vertical-align: middle;
  }
}

.btn--narrow {
  padding-left: 15px;
  padding-right: 15px;
}

.btn--disabled {
  opacity: 0.5;
  cursor: default;

  &:hover,
  &:focus {
    opacity: 0.5;
  }
}

.btn--has-icon-after {
  .icon {
    margin-left: 10px;
  }
}

.btn--has-icon-before {
  .icon {
    margin-right: 10px;
  }
}

// Return to collection/blog links
.return-link-wrapper {
  margin: ($gutter-site * 1.5) 0;

  @include media-query($small) {
    margin-bottom: (-$section-spacing) - 1; // same as .site-footer

    .btn {
      display: block;
    }
  }
}

/// Tables
table {
  margin-bottom: $gutter-site / 2;
}

th {
  font-family: $font-stack-header;
  font-weight: $font-weight-bold;
}

th,
td {
  text-align: left;
  border: 1px solid $color-border;
  padding: 10px 14px;
}

/* ===========================================================
   Responsive tables - defined with .responsive-table on table element
 * ===========================================================*/

@include media-query($small) {
  .responsive-table {
    thead {
      display: none;
    }

    tr {
      display: block;
    }

    // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      text-align: right;
      padding: 10px 15px;
      border: 0;
      margin: 0;
    }

    td::before {
      content: attr(data-label);
      float: left;
      text-align: center;
      font-size: 12px;
      padding-right: 10px;
    }
  }

  .responsive-table__row + .responsive-table__row,
  tfoot > .responsive-table__row:first-child {
    position: relative;
    margin-top: 10px;
    padding-top: $gutter-site;

    &::after {
      content: '';
      display: block;
      position: absolute;
      top: 0;
      left: $gutter-site / 2;
      right: $gutter-site / 2;
