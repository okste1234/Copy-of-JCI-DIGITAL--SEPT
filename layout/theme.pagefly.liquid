<script type="text/javascript">
  var req = new XMLHttpRequest();
  req.overrideMimeType("application/json");
  req.open('GET', '/browsing_context_suggestions.json', true);
  req.onload  = function() {
    var jsonResponse = JSON.parse(req.responseText);
    document.body.setAttribute('data-country-handle', jsonResponse.detected_values.country.handle);
    document.body.setAttribute('data-country-name', jsonResponse.detected_values.country.name);
  };
  req.send(null);
</script>
